***fibonacci series
#include<stdio.h>
int fib(int n)
{
	if(n<=0)
	{
		return 0;
	}
	else if(n==1)
	{
		return 1;
	}
	else 
	{
		return fib(n-1)+fib(n-2);
	}
}
int main()
{
	int i,n;
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
		printf("%d ",fib(i));
	}
 _________________________________________________________________________________________________________________________________________________________________________
***Implement a 2D array and rotate the matrix in 90 degree

	#include <stdio.h>

	void main()

	{
	int n,i,j,temp; n=2;	
	scanf("%d",&n);
	int a[20][20];
	for(i=0;i<n;i++)
	{
	for(j=0;j<n;j++)
	{
	scanf("%d",&a[i][j]);
	}
	}

	for(i=0;i<n;i++)
	{
	for(j=n-1;j>=0;j--)
	{
	printf("%d ",a[j][i]);
	}
	printf("\n");
	}
	}
